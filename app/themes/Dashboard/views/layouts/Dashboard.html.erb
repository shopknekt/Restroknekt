<!DOCTYPE html>
<html>
<head>
  <title>Dashboard</title>
  <%= stylesheet_link_tag    "Dashboard/all", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "Dashboard/all", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div>
      <ul>
          <li><%= link_to "Home", root_path %></li>
          <% if current_shop_admin.present? %>
            <%= link_to "Sign Out", destroy_shop_admin_session_path, method: :delete, :id => "button" %></li>
          <% else %>
            <li><%= link_to "Register", new_shop_admin_registration_path %></li>
            <li><%= link_to "Sign In", new_shop_admin_session_path %></li>
          <% end %>
      </ul>
    </div>
<%= yield %>

</body>
</html>